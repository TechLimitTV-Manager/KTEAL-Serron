        <ListView x:ConnectionId='4' x:Name="Line9_List" Margin="10,193,10,10"/>
        <Button x:ConnectionId='5' x:Name="Line9_Daily_Button" Content="Καθημερινή" HorizontalAlignment="Left" Margin="10,156,0,0" VerticalAlignment="Top"                                  Width="119"/>
        <Button x:ConnectionId='6' x:Name="Line9_Saturday_Button" Content="Σάββατο" HorizontalAlignment="Left" Margin="134,156,0,0" VerticalAlignment="Top"                                     Width="85"/>
        <Button x:ConnectionId='7' x:Name="Line9_Sunday_Button" Content="Κυριακή" HorizontalAlignment="Left" Margin="224,156,0,0" VerticalAlignment="Top"                                   Width="126"/>
        <TextBlock x:ConnectionId='8' x:Name="Line9_Title" Margin="10,115,10,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="36" TextAlignment="Center"/>
    </Grid>
</Page>

